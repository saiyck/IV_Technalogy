{
  "name": "shankar_narayana",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "apk": "cd android  && gradlew.bat assembleRelease && cd ..",
    "dev-install": "react-native run-android && adb reverse tcp:8081 tcp:8081 && npm start",
    "apk-install": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "debug:64": "keytool -exportcert -alias androiddebugkey -keystore debug.keystore | openssl sha1 -binary | openssl base64",
    "prod:64": "keytool -exportcert -alias certitude_prod -keystore certitude_prod.keystore | openssl sha1 -binary | openssl base64",
    "debug:sha": "keytool -list -v -keystore ./android/app/debug.keystore -alias androiddebugkey -storepass android -keypass android",
    "prod:sha": "keytool -list -v -keystore ./android/app/certitude_prod.keystore -alias certitude_prod -storepass CertitudeProd@5001 -keypass CertitudeProd@5001",
    "android:clean": "cd android && gradlew.bat clean && cd .."
  },
